{
  "name": "selenium-automation",
  "version": "1.0.0",
  "description": "Selenium Automation with React UI",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "ui:install": "cd ui && npm install",
    "ui:build": "cd ui && npm run build",
    "ui:dev": "cd ui && npm run dev",
    "dev": "concurrently \"npm run ui:dev\" \"nodemon server.js\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
