{
  "name": "selenium-automation-for-owner-name",
  "version": "1.0.0",
  "private": true,
  "description": "Selenium Automation with React UI",
  "type": "module",
  "main": "server.js",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "cd simple_ui && npm run build",
    "build:ui": "cd simple_ui && npm ci && npm run build",
    "vercel-build": "npm install --omit=dev && npm run build:ui",
    "deploy": "npm run validate && vercel --prod",
    "validate": "node scripts/validate-all.cjs",
    "validate:json": "node scripts/validate-json.cjs",
    "validate:vercel": "node scripts/validate-vercel-schema.cjs",
    "validate:node": "node scripts/validate-node-version.cjs",
    "validate:packages": "node scripts/validate-vercel-packages.cjs",
    "validate:build": "node scripts/validate-build-config.cjs",
    "validate:routing": "node scripts/validate-routing-404.cjs",
    "test:routing": "node scripts/test-routing-404.cjs",
    "precommit": "node scripts/pre-commit-check.cjs"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-validator": "^7.3.0",
    "multer": "^2.0.2"
  },
  "devDependencies": {
    "@vercel/node": "^5.5.5",
    "@vercel/static-build": "^2.8.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0",
    "nodemon": "^3.0.2",
    "vercel": "^32.2.5"
  },
  "packageManager": "npm@10.2.3"
}
