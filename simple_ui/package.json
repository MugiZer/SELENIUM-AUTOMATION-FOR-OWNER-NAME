{
  "name": "property-scraper-ui",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "vercel-build": "npm run build && npm run verify-build",
    "verify-build": "node -e \"const fs = require('fs'); if (!fs.existsSync('dist/index.html')) { console.error('❌ Build failed: index.html not found!'); process.exit(1); } else { console.log('✅ Build verification passed'); }\""
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@types/react-dropzone": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.9.5"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
